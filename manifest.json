{
    "name": "Absent, Excused",
    "version": "1.0",
    "description": "Adds absent, excused update all function",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "attendance.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "https://wss.uwinnipeg.ca/*"
            ],
            "js": [
                "attendance.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}